import java.util.Scanner;

public class A {
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		System.out.println("무슨 커피를 드릴까요?: ");
		String coffee = scan.nextLine();
		int price;
		switch (coffee) {
		case "에스프레소" :
		case "카푸치노" :
		case "카페라떼" :
			System.out.println("가격은 3500원입니다.");
			price = 3500;
		case "아메리카노" :
			System.out.println("가격은 2500원입니다");
			price = 2500;
		default :
			System.out.println("메뉴에 없습니다.");
			price = 0;
		}
		if(!=0)
			

		scan.close();

	}
}
