import java.util.Scanner;

public class A {
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		int number = scan.nextInt();
		if(number % 6 == 0) {
			System.out.println("6의 배수입니다");
		}
		else if(number % 2 == 1) {
			System.out.println("홀수입니다");
		}
		scan.close();
	}
}
